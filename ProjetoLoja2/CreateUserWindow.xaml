<Window x:Class="ProjectStore.CreateUserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectStore"
        mc:Ignorable="d"
        Title="createUser" Height="450" Width="464">
    <Grid>
        <Label x:Name="label_user_title" Content="Usuário" HorizontalAlignment="Left" Margin="204,82,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textboxNewUser" HorizontalAlignment="Left" Height="22" Margin="164,113,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_password_title" Content="Senha" HorizontalAlignment="Left" Margin="204,140,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.276,0.212"/>
        <TextBox x:Name="textbox_new_password" HorizontalAlignment="Left" Height="23" Margin="164,166,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="buttonNewUser" Content="Criar" HorizontalAlignment="Left" Margin="188,194,0,0" VerticalAlignment="Top" Width="74" Height="29" Click="buttonNewUser_Click"/>

    </Grid>
</Window>
